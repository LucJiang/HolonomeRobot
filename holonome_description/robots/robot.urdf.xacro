<?xml version="1.0" encoding="utf-8"?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="base_link">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/Bati.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="${pi/2} 0 ${pi/2 + 2*pi/3}"/>
    </visual>

    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/Bati.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="${pi/2} 0 ${pi/2 + 2*pi/3}"/>
    </collision>

    <!--
    <inertial>
      <origin xyz="0.0 0 0."/>
      <mass value="0.5"/> 
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                iyy="0.01" iyz="0.0" 
                izz="0.03" />
    </inertial>
    -->
  </link>

  

  <link name="wheel_front">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/2} 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="117.2 0 -21.4" rpy="0 ${pi/2} 0"/>
    </collision>
    <!--
    <inertial>
      <origin xyz="0.0 0 0."/>
      <mass value="0.5"/> 
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                iyy="0.01" iyz="0.0" 
                izz="0.03" />
    </inertial>
    -->
  </link>

  <joint name="wheel_front_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_front"/>
    <origin xyz="117.2 0 -21.4" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="rouleau_front1_joint" type="continuous">
    <parent link="wheel_front"/>
    <child link="rouleau_front1"/>
    <origin xyz="7.4 12.24 -12.24" rpy="0 0 0"/>
    <axis xyz="0 1 1"/>
  </joint>
  <link name="rouleau_front1">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/4} ${-pi/2}"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/4} ${-pi/2}"/>
    </collision>
  </link>

  <joint name="rouleau_front2_joint" type="continuous">
    <parent link="wheel_front"/>
    <child link="rouleau_front2"/>
    <origin xyz="7.4 -12.24 -12.24" rpy="0 0 0"/>
    <axis xyz="0 1 -1"/>
  </joint>
  <link name="rouleau_front2">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${-pi/4} ${-pi/2}"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${-pi/4} ${-pi/2}"/>
    </collision>
  </link>

  <joint name="rouleau_front3_joint" type="continuous">
    <parent link="wheel_front"/>
    <child link="rouleau_front3"/>
    <origin xyz="7.4 -12.24 12.24" rpy="0 0 0"/>
    <axis xyz="0 1 1"/>
  </joint>
  <link name="rouleau_front3">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/4} ${-pi/2}"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/4} ${-pi/2}"/>
    </collision>
  </link>

  <joint name="rouleau_front4_joint" type="continuous">
    <parent link="wheel_front"/>
    <child link="rouleau_front4"/>
    <origin xyz="7.4 12.24 12.24" rpy="0 0 0"/>
    <axis xyz="0 1 -1"/>
  </joint>
  <link name="rouleau_front4">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${-pi/4} ${-pi/2}"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/rouleau.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 ${pi/4} ${-pi/2}"/>
    </collision>
  </link>
  

  <link name="wheel_left">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="-58.6 101.5 -21.4" rpy="0 ${pi/2} ${pi*2/3}"/>
    </collision>
    <!--
    <inertial>
      <origin xyz="0.0 0 0."/>
      <mass value="0.5"/> 
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                iyy="0.01" iyz="0.0" 
                izz="0.03" />
    </inertial>
    -->
  </link>

  <joint name="wheel_left_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_left"/>
    <origin xyz="-58.6 101.5 -21.4" rpy="0 ${pi/2} ${pi*2/3}"/>
    <axis xyz="${cos(pi*2/3)} ${sin(pi*2/3)} 0"/>
  </joint>

  

  <link name="wheel_right">
    <visual>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://holonome_description/meshes/roue.stl"/>
      </geometry>
      <origin xyz="-58.6 -101.5 -21.4" rpy="0 ${pi/2} ${-pi*2/3}"/>
    </collision>
    <!--
    <inertial>
      <origin xyz="0.0 0 0."/>
      <mass value="0.5"/> 
      <inertia ixx="0.01" ixy="0.0" ixz="0.0"
                iyy="0.01" iyz="0.0" 
                izz="0.03" />
    </inertial>
    -->
  </link>
  
  <joint name="wheel_right_joint" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_right"/>
    <origin xyz="-58.6 -101.5 -21.4" rpy="0 ${pi/2} ${-pi*2/3}"/>
    <axis xyz="${cos(pi*2/3)} ${sin(pi*2/3)} 0"/>
  </joint>

</robot>